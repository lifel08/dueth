<div class="nested-fields">
	<div class="form-group row">
		<label for="staticEmail" class="col-sm-2 col-form-label">Start Datetime</label>
		<div class="col-sm-10">
		 <%= f.datetime_local_field :start_datetime, class: 'form-control', name: "start_date" %>
		</div>
	</div>
		<div class="form-group row">
			<label for="inputPassword" class="col-sm-2 col-form-label">End Datetime</label>
			<div class="col-sm-10">
			 <%= f.datetime_local_field :end_datetime, class: 'form-control', name: "end_date" %>
			</div>
	</div>
	<%= link_to_remove_association 'Remove', f, class:'btn btn-danger' %>
</div>

<script>
	var today = new Date().toISOString().slice(0, 16);
	document.getElementsByName("start_date")[0].min = today;

	var today = new Date().toISOString().slice(0, 16);
	document.getElementsByName("end_date")[0].min = today;
</script>